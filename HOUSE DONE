local v2 = v1:CreateWindow("Protect The House")
v2:AddButton({
    text = "Inf Ammo",
    flag = "button",
    callback = function()
        local vu3 = nil
        local _ = hookmetamethod(game, "__index", function(p4, p5)
            if checkcaller() or (tostring(p4) ~= "Mag" or p5 ~= "Value") then
                return vu3(p4, p5)
            else
                return math.huge
            end
        end)
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end
})

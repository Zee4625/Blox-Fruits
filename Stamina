local StaminaOnoff= false
local EspOnOff = false
local HealthOnOff = false
local Heartbeat1
local XOnOff = true
local MousekillOnOff = false

Stamina.MouseButton1Up:Connect(function()
	
	if StaminaOnoff == false then
		StaminaOnoff = true
		Stamina.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		Stamina.Text = "Stamina: On"
		

		local runservice = game:GetService("RunService")

	Heartbeat1 = runservice.Heartbeat:Connect(function()
		
			local Characters = game.Workspace:FindFirstChild("Characters")

			local player = game.Players.LocalPlayer

			

			

				local Attributes = player.Character:FindFirstChild("Attributes")
				local Stamina = Attributes:GetAttribute("Stamina")
				local Thirst = Attributes:GetAttribute("Thirst")

				if Stamina < 100 then
					Attributes:SetAttribute("Stamina",100)


				end
				if Thirst < 70 then
					Attributes:SetAttribute("Thirst",70)
				end
			end)

			

	
	



	elseif StaminaOnoff == true then
		Stamina.BackgroundColor3 = Color3.fromRGB(212,212,212)
		Stamina.Text = "Stamina: off"
		StaminaOnoff = false
		Heartbeat1:Disconnect()

		local Characters = game.Workspace:FindFirstChild("Characters")

		local player = game.Players.LocalPlayer





		local Attributes = player.Character:FindFirstChild("Attributes")
		local Stamina = Attributes:GetAttribute("Stamina")
		local Thirst = Attributes:GetAttribute("Thirst")

		
			Attributes:SetAttribute("Stamina",100)
			Attributes:SetAttribute("Thirst",90)
		
		
		
	end
	
end)

local Heartbeat2

Health.MouseButton1Up:Connect(function()

	if HealthOnOff == false then
		HealthOnOff = true
		Health.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		Health.Text = "Health: On"


		local runservice = game:GetService("RunService")

		Heartbeat2 = runservice.Heartbeat:Connect(function()

			


			local args = {
				[1] = 2.604166666666667,
				[2] = "iryan96"
			}

			local Player = game.Players.LocalPlayer

				
				local Character = Player.Character

				local Humanoid = Character:FindFirstChild("Humanoid")

				if Humanoid.Health < 100 then



					game.ReplicatedStorage.Events.TakeDamage:FireServer(-1,"iryan96")

				end
			

		end)





	elseif HealthOnOff == true then
		Health.BackgroundColor3 = Color3.fromRGB(212,212,212)
		Health.Text = "Health: off"
		HealthOnOff = false
		Heartbeat2:Disconnect()
		local Player = game.Players.LocalPlayer


		local Character = Player.Character

		local Humanoid = Character:FindFirstChild("Humanoid")
		
		
		game.ReplicatedStorage.Events.TakeDamage:FireServer(0.11,"iryan96")
        

	end

end)

local Heartbeat3

Esp.MouseButton1Up:Connect(function()

	if EspOnOff == false then
		EspOnOff = true
		Esp.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		Esp.Text = "Esp: On"


		local runservice = game:GetService("RunService")

		Heartbeat3 = runservice.Heartbeat:Connect(function()
			local Players = game:GetService("Players"):GetChildren()
			local RunService = game:GetService("RunService")
			local highlight = Instance.new("Highlight")
			highlight.Name = "Highlight"







			

				local charactersfolder = game.Workspace:FindFirstChild("Characters")

				for i, v in pairs(charactersfolder:GetChildren()) do
					if v.Name ~= "HighlightedPlayers" then

						if v:FindFirstChild("HumanoidRootPart") then
							local HumanoidRootPart = v:FindFirstChild("HumanoidRootPart")

							if not HumanoidRootPart:FindFirstChild("Highlight") then
								local highlightClone = highlight:Clone()
								highlightClone.Adornee = v
								highlightClone.Parent = HumanoidRootPart
								highlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
								highlightClone.Name = "Highlight"
								task.wait()
							end
						end

					end
				end
		

		end)





	elseif EspOnOff == true then
		Esp.BackgroundColor3 = Color3.fromRGB(212,212,212)
		Esp.Text = "Esp: off"
		EspOnOff = false
		Heartbeat3:Disconnect()
		local charactersfolder = game.Workspace:FindFirstChild("Characters")
		local Players = game:GetService("Players"):GetChildren()
		
		for i, v in pairs(charactersfolder:GetChildren()) do
			
			if v.Name ~= "HighlightedPlayers" then
				
				 if v:FindFirstChild("HumanoidRootPart") then
						local HumanoidRootPart = v:FindFirstChild("HumanoidRootPart")
						
						if HumanoidRootPart:FindFirstChild("Highlight") then
							local Highlightfound = HumanoidRootPart:FindFirstChild("Highlight")
							Highlightfound:Destroy()
						end
						
				 end
			end

		end

	end

end)

Xbutton.MouseButton1Up:Connect(function()
	
	if XOnOff == false then
		
		ScreenGui.Enabled = true
		XOnOff = true
		Xbutton.BackgroundColor3 = Color3.fromRGB(255,0,0)
		Xbutton.Text = "X"
		
	elseif XOnOff == true then
		
		ScreenGui.Enabled = false
		XOnOff = false
		Xbutton.BackgroundColor3 = Color3.fromRGB(0, 255, 30)
		Xbutton.Text = "Open"
	end
	
end)



Death.MouseButton1Up:Connect(function()
	

		
	if DeathCanBeUsed == true then
		local Player = game.Players.LocalPlayer


		local Character = Player.Character

		local Humanoid = Character:FindFirstChild("Humanoid")
	   
	   repeat task.wait()
			
			


			game.ReplicatedStorage.Events.TakeDamage:FireServer(4,"iryan96")
			
	   until Humanoid.Health <= 0
	   Death.BackgroundColor3 = Color3.fromRGB(212,212,212)
	   DeathCanBeUsed = false
		
	end
end)

local RunService = game:GetService("RunService")

RunService.Heartbeat:Connect(function()
	
	local Player = game.Players.LocalPlayer
	
	
	repeat task.wait()
		
		
	until Player.Character
	local Character = Player.Character
	local Humanoid = Character:FindFirstChild("Humanoid")
	local CharactersFolder = game.Workspace:FindFirstChild("Characters")
	
	if Player.Character:FindFirstChild("HumanoidRootPart") and Humanoid.Health > 0 and Character.Parent == CharactersFolder then
		DeathCanBeUsed = true
		Death.BackgroundColor3 = Color3.fromRGB(255,0,0)
		
	else
		
		DeathCanBeUsed = false
		Death.BackgroundColor3 = Color3.fromRGB(212,212,212)
	end
	
end)

local MouseRuntime 

MouseKill.MouseButton1Up:Connect(function()
	
	if MousekillOnOff == false and DeathCanBeUsed == true then
		MousekillOnOff = true
		MouseKill.BackgroundColor3 = Color3.fromRGB(0,255,0)
		MouseKill.Text = "MouseKill: On"
		
	elseif MousekillOnOff == true then
		
		MousekillOnOff = false
		MouseKill.BackgroundColor3 = Color3.fromRGB(212,212,212)
		MouseKill.Text = "MouseKill: Off"
		
	end
	
	
end)


local Userinputservice = game:GetService("UserInputService")


function FindHeadHitbox(character)
	
	local Animal = character:GetAttribute("Animal")
	
	if Animal == "Bison" then
		return character:FindFirstChild("HeadHitbox")
		
	elseif Animal == "DireWolf" then
		return character:FindFirstChild("HeadHitbox")
	elseif Animal == "Smilodon" then
		return character:FindFirstChild("HeadHitbox")
		
	elseif Animal == "WesternHorse" then
		
		for i, v in pairs(character:GetDescendants()) do
			if v.Name == "Neck03" then
				if v.Parent.Name == "HeadHitbox" then
					
					return v.Parent
				end
			end
			
		end
		--
			
	else
		
		return nil
	end
	
	
end

local function AddtoTable(T,T2)
	
	local Descendants = T:GetDescendants()
	for i, v in pairs(Descendants) do
		
		table.insert(T2, v)
	end
	
	
	
end


local function WhitelistforRaycast(T)
	local Blacklist = {}
	
end



local function raycastTarget(a,t,folder)
	local direction = (t.Position - a.Position)
	local IgnoreTable = {a.Parent}
	local Blacklistparams = RaycastParams.new()
	Blacklistparams.FilterDescendantsInstances = {IgnoreTable}
	Blacklistparams.FilterType = Enum.RaycastFilterType.Exclude
	Blacklistparams.IgnoreWater = true
	
	local Finished = false

	
	local Hits = {}
	
	repeat
		if MousekillOnOff == false then
			return nil
		end
		Blacklistparams.FilterDescendantsInstances = {IgnoreTable}
		
		local Raycast = workspace:Raycast(a.Position, direction, Blacklistparams)
		
		if Raycast then
			
			if Raycast.Instance then
				
				if Raycast.Instance.Parent ~= a.Parent then
					if Raycast.Instance.Parent.Parent == folder and Raycast.Instance.Parent.Name ~= "HighlightedPlayers" then
						
						return Raycast.Instance
					else
						table.insert(IgnoreTable, Raycast.Instance)
						table.insert(Hits, Raycast.Instance)
						task.wait()
					end
					
				else
					
					table.insert(IgnoreTable, Raycast.Instance)
					table.insert(Hits, Raycast.Instance)
					task.wait()
				end
			else
				Finished = true
				return nil
			end--
		
		end--
		task.wait()
			
	until Finished == true --
	
	
	
	


	
end

Userinputservice.InputBegan:Connect(function(input, gameProcessedEvent)
	
	if not gameProcessedEvent then
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			if MousekillOnOff == true then
				
				 
				
					local Player = game.Players.LocalPlayer
					local mouse = Player:GetMouse()
					local CharactersFolder = game.Workspace:FindFirstChild("Characters")
					
					if DeathCanBeUsed == true then
						local Target = mouse.Target
					local Ourcharacter = Player.Character
						
						
						
						if Target ~= nil then
							local RaycastTarget = raycastTarget(Ourcharacter:FindFirstChild("HumanoidRootPart"),Target,CharactersFolder)
							
							if RaycastTarget ~= nil and RaycastTarget.Parent.Parent == CharactersFolder and RaycastTarget.Parent.Name ~= "HighlightedPlayers" then
							   local CharacterTarget = RaycastTarget.Parent
							   local HeadHitbox = FindHeadHitbox(CharacterTarget)
							
									
				
							   
							local args = {
								[1] = CharacterTarget.Humanoid,
								[2] = HeadHitbox,
								[3] = 2
							}

							game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Attack"):FireServer(unpack(args))
                                  end
						
							
						end
						
					end
					
					
			
			end
			
		end
		
		
	end
	
	
end)

local function GetPlayer(name)
	
	name = name:lower()
	
	
	for i, v in pairs(game.Players:GetChildren()) do
		
		if name == v.Name:lower():sub(1,#name) then
			return v
		end
	end
	
	return nil
	
end

local CooldownTp = false


Tp.FocusLost:Connect(function(enterpressed)
	 if enterpressed and CooldownTp == false and DeathCanBeUsed == true then
		
		local TextEntered = Tp.Text
		local PlayerTarget = GetPlayer(TextEntered)
		
		local Player = game.Players.LocalPlayer
		local Character = Player.Character
		local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
		if PlayerTarget ~= nil then
			
		if PlayerTarget.Character then
			local CharacterTarget = PlayerTarget.Character
			local RootHum = CharacterTarget:FindFirstChild("HumanoidRootPart")
			if RootHum then
		


		HumanoidRootPart.CFrame = RootHum.CFrame * CFrame.new(70,48,70)
		
		CooldownTp = true
		Tp.Text = "Tp Cooldown..."
		Tp.BackgroundColor3 = Color3.fromRGB(255,0,0)
		task.wait(1.5)
		CooldownTp = false
		Tp.BackgroundColor3 = Color3.fromRGB(212,212,212)
		Tp.Text = "Tp to:..."
		        end
		    end
		end
	 else
			
			
			Tp.Text = "Tp to:..."
		Tp.BackgroundColor3 = Color3.fromRGB(212,212,212)
	 
	 
		
		
		end
	
end)

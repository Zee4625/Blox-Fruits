--Properties:

Tp.Name = "Tp"
Tp.Parent = ScrollingFrame
Tp.BackgroundColor3 = Color3.fromRGB(212, 212, 212)
Tp.BorderColor3 = Color3.fromRGB(0, 0, 0)
Tp.BorderSizePixel = 2
Tp.LayoutOrder = 6
Tp.Size = UDim2.new(0, 200, 0, 50)
Tp.Font = Enum.Font.SourceSans
Tp.Text = "Tp to:..."
Tp.TextColor3 = Color3.fromRGB(0, 0, 0)
Tp.TextScaled = false
Tp.TextSize = 100

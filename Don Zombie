buttonCerc.Text = "Zombie Run"
buttonCerc.TextColor3 = Color3.new(255, 255, 255)
buttonCerc.BackgroundColor3 = Color3.new(63 / 255, 0, 63 / 255)
buttonCerc.TextScaled = true
buttonCerc.Font = Enum.Font.SourceSansBold


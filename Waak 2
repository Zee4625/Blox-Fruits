PlayerSection:NewButton("Noclip (not a toggle)", "Lets you noclip", function()
--[[
	WARNING: Heads up! This script has not been verified by ScriptBlox. Use at your own risk!
]]
local Noclip = nil
local Clip = nil

function noclip()
	Clip = false
	local function Nocl()
		if Clip == false and game.Players.LocalPlayer.Character ~= nil then
			for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
				if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
					v.CanCollide = false
				end
			end
		end
		wait(0.21) -- basic optimization
	end
	Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
	if Noclip then Noclip:Disconnect() end
	Clip = true
end

noclip() -- to toggle noclip() and clip()
end)

MainSection:NewButton("Auto Breathing", "no shit", function()
local KeyToPress = Enum.KeyCode.G  -- Change to any key

-- Services
local UserInputService = game:GetService("UserInputService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local TweenService = game:GetService("TweenService")

-- GUI Setup
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local ToggleButton = Instance.new("TextButton")
local ModeButton = Instance.new("TextButton")
local SpeedSlider = Instance.new("TextBox")
local SliderLabel = Instance.new("TextLabel")
local CloseButton = Instance.new("TextButton")

ScreenGui.Parent = game.CoreGui
ScreenGui.ResetOnSpawn = false

MainFrame.Size = UDim2.new(0, 280, 0, 220)
MainFrame.Position = UDim2.new(0.5, -140, 0.5, -110)
MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundColor3 = Color3.fromRGB(255, 0, 100)
Title.Text = "Max Hub | Auto Breathing"
Title.TextColor3 = Color3.new(1, 1, 1)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 16
Title.Parent = MainFrame

CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Position = UDim2.new(1, -30, 0, 0)
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.new(1, 1, 1)
CloseButton.Font = Enum.Font.GothamBold
CloseButton.Parent = MainFrame

ToggleButton.Size = UDim2.new(0, 200, 0, 50)
ToggleButton.Position = UDim2.new(0.5, -100, 0, 40)
ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 100)
ToggleButton.Text = "OFF"
ToggleButton.TextColor3 = Color3.new(1, 1, 1)
ToggleButton.Font = Enum.Font.GothamBold
ToggleButton.TextSize = 20
ToggleButton.Parent = MainFrame

ModeButton.Size = UDim2.new(0, 200, 0, 40)
ModeButton.Position = UDim2.new(0.5, -100, 0, 100)
ModeButton.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
ModeButton.Text = "CLICK MODE"
ModeButton.TextColor3 = Color3.new(1, 1, 1)
ModeButton.Font = Enum.Font.GothamBold
ModeButton.TextSize = 14
ModeButton.Parent = MainFrame

SliderLabel.Size = UDim2.new(0, 200, 0, 20)
SliderLabel.Position = UDim2.new(0.5, -100, 0, 150)
SliderLabel.BackgroundTransparency = 1
SliderLabel.Text = "Delay: 0.05s"
SliderLabel.TextColor3 = Color3.new(1, 1, 1)
SliderLabel.Font = Enum.Font.Gotham
SliderLabel.Parent = MainFrame

SpeedSlider.Size = UDim2.new(0, 200, 0, 30)
SpeedSlider.Position = UDim2.new(0.5, -100, 0, 175)
SpeedSlider.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
SpeedSlider.Text = "0.05"
SpeedSlider.TextColor3 = Color3.new(1, 1, 1)
SpeedSlider.Font = Enum.Font.Gotham
SpeedSlider.Parent = MainFrame

-- Variables
local Clicking = false
local Delay = 0.05
local HoldMode = false  -- false = Click, true = Hold
local HoldConnection = nil

-- Functions
local function PressKey()
    VirtualInputManager:SendKeyEvent(true, KeyToPress, false, game)
end

local function ReleaseKey()
    VirtualInputManager:SendKeyEvent(false, KeyToPress, false, game)
end

local function StartClicking()
    Clicking = true
    ToggleButton.Text = HoldMode and "HOLDING" or "CLICKING"
    ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    
    if HoldMode then
        -- HOLD MODE: Press once and keep held
        PressKey()
    else
        -- CLICK MODE: Spam press/release
        task.spawn(function()
            while Clicking do
                PressKey()
                task.wait(0.01)
                ReleaseKey()
                task.wait(Delay)
            end
        end)
    end
end

local function StopClicking()
    Clicking = false
    ReleaseKey()  -- Always release on stop
    ToggleButton.Text = "OFF"
    ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 100)
end

-- Events
ToggleButton.MouseButton1Click:Connect(function()
    if Clicking then
        StopClicking()
    else
        StartClicking()
    end
end)

ModeButton.MouseButton1Click:Connect(function()
    HoldMode = not HoldMode
    ModeButton.Text = HoldMode and "HOLD MODE" or "CLICK MODE"
    ModeButton.BackgroundColor3 = HoldMode and Color3.fromRGB(255, 150, 0) or Color3.fromRGB(0, 150, 255)
    
    if Clicking then
        StopClicking()
        task.wait(0.1)
        StartClicking()
    end
end)

SpeedSlider.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local num = tonumber(SpeedSlider.Text)
        if num and num > 0 then
            Delay = num
            SliderLabel.Text = "Delay: " .. num .. "s"
        else
            SpeedSlider.Text = tostring(Delay)
        end
    end
end)

CloseButton.MouseButton1Click:Connect(function()
    StopClicking()
    ScreenGui:Destroy()
end)

-- Start OFF
StopClicking()
print("Auto Breathing")
end)

     MainSection:NewButton("ESP", "Shows all people", function()
--============================--
-- Developer ESP (No UI, With Distance + Name)
--============================--

local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer
local charactersFolder = workspace:WaitForChild("CharactersAndNPCs")
local espObjects = {}

-- Cleanup old ESP if script is run again
for _, obj in ipairs(espObjects) do
    if obj and obj.Parent then
        obj:Destroy()
    end
end
espObjects = {}

local function addESP(model)
    if not model:IsA("Model") then return end

    local humanoid = model:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end

    ---------------------------------------------------
    -- Highlight
    ---------------------------------------------------
    local highlight = Instance.new("Highlight")
    highlight.Adornee = model
    highlight.FillColor = Color3.fromRGB(0, 255, 0)
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
    highlight.Parent = model
    table.insert(espObjects, highlight)

    ---------------------------------------------------
    -- Billboard for name, health, and distance
    ---------------------------------------------------
    local head = model:FindFirstChild("Head") or model:FindFirstChildWhichIsA("BasePart")
    if not head then return end

    local billboard = Instance.new("BillboardGui")
    billboard.Size = UDim2.new(0, 150, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 4, 0)
    billboard.AlwaysOnTop = true
    billboard.Adornee = head
    billboard.Parent = model

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
    label.TextStrokeTransparency = 0
    label.TextScaled = true
    label.Parent = billboard

    -- Initial text
    label.Text = model.Name .. " | HP: " .. math.floor(humanoid.Health)

    -- Update health
    humanoid.HealthChanged:Connect(function()
        label.Text = model.Name .. " | HP: " .. math.floor(humanoid.Health)
    end)

    ---------------------------------------------------
    -- Distance updater loop
    ---------------------------------------------------
    task.spawn(function()
        while billboard.Parent do
            task.wait(0.1)

            local char = localPlayer.Character
            local hrp = char and char:FindFirstChild("HumanoidRootPart")
            if not hrp then
                label.Text = model.Name .. " | HP: " .. math.floor(humanoid.Health) .. " | Dist: --"
                continue
            end

            local distance = (head.Position - hrp.Position).Magnitude
            label.Text = model.Name .. " | HP: " .. math.floor(humanoid.Health) .. " | Dist: " .. math.floor(distance)
        end
    end)

    table.insert(espObjects, billboard)
end

---------------------------------------------------
-- Add ESP to existing models
---------------------------------------------------
for _, model in ipairs(charactersFolder:GetChildren()) do
    addESP(model)
end

---------------------------------------------------
-- Auto-ESP new models
---------------------------------------------------
charactersFolder.ChildAdded:Connect(function(child)
    task.wait(0.1)
    addESP(child)
end)





end)

    VisualSection:NewButton("All stuff", "Gives all items", function()
    local Players = game:GetService("Players")

local function updatePlayer(player)
    local inventory = player:WaitForChild("Inventory", 5)
    if not inventory then
        warn("Inventory not found for player:", player.Name)
        return
    end

    -- Loop through all children in Inventory
    for _, item in ipairs(inventory:GetChildren()) do
        if item:IsA("IntValue") or item:IsA("NumberValue") then
            item.Value = 100
        else
            warn("Skipped non-number value in Inventory:", item.Name)
        end
    end
end

-- Update all players currently in the game
for _, player in ipairs(Players:GetPlayers()) do
    updatePlayer(player)
end

-- Update players who join later
Players.PlayerAdded:Connect(updatePlayer)



end)

    VisualSection:NewButton("100 Million Max Breathing", "Gives 100M Max breathing", function()
    local Players = game:GetService("Players")

local function updatePlayer(player)
    local breathingSettings = player:WaitForChild("BreathingSettings", 5)
    if breathingSettings then
        local maxBreathing = breathingSettings:FindFirstChild("MaxBreathing")
        if maxBreathing then
            maxBreathing.Value = 100000000
        else
            warn("MaxBreathing value not found for player:", player.Name)
        end
    else
        warn("BreathingSettings not found for player:", player.Name)
    end
end

-- Update all current players
for _, player in ipairs(Players:GetPlayers()) do
    updatePlayer(player)
end

-- Update players who join later
Players.PlayerAdded:Connect(updatePlayer)

end)

    VisualSection:NewButton("Max Level", "Gives level 1650", function()
    local Players = game:GetService("Players")

local function updatePlayer(player)
    local dataFolder = player:WaitForChild("Data", 5)
    if dataFolder then
        local levels = dataFolder:FindFirstChild("Levels")
        if levels then
            levels.Value = 1650
        else
            warn("Levels value not found for player:", player.Name)
        end
    else
        warn("Data folder not found for player:", player.Name)
    end
end

-- Update all players currently in the game
for _, player in ipairs(Players:GetPlayers()) do
    updatePlayer(player)
end

-- Update players who join later
Players.PlayerAdded:Connect(updatePlayer)


end)



    ChristmasEventSection:NewButton("Gift Teleport", "Teleports you to a random gift in the Christmas event", function()
   local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

local giftsFolder = workspace:WaitForChild("Map"):WaitForChild("Gifts")

-- Collect all parts inside the folder
local parts = {}
for _, obj in ipairs(giftsFolder:GetDescendants()) do
	if obj:IsA("BasePart") then
		table.insert(parts, obj)
	end
end

-- Teleport to a random one
if #parts > 0 then
	local randomPart = parts[math.random(1, #parts)]
	hrp.CFrame = randomPart.CFrame + Vector3.new(0, 3, 0) -- slightly above it
else
	warn("No parts found inside workspace.Map.Gifts")
end

end)

    PlayerSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
            currentSpeed = v
end)

-- Re-apply walkspeed every frame (fixes animation overrides)
RunService.RenderStepped:Connect(function()
    local char = player.Character
    if char then
        local hum = char:FindFirstChildWhichIsA("Humanoid")
        if hum and hum.WalkSpeed ~= currentSpeed then
            hum.WalkSpeed = currentSpeed
        end
    end
end)


function Kill()
    spawn(function()
        _G.Kill = true
        while _G.Kill do
            wait()
            pcall(function()
                local v4 = game:GetService("Players").LocalPlayer
                local v5 = (v4.Character or v4.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
                local v6, v7, v8 = pairs(workspace:GetChildren())
                while true do
                    local v9
                    v8, v9 = v6(v7, v8)
                    if v8 == nil then
                        break
                    end
                    if v9:IsA("Model") and (v9:GetAttribute("hadEntrance") and (v9:FindFirstChild("HumanoidRootPart") and (v9.HumanoidRootPart.Position - v5.Position).Magnitude <= 30)) then
                        game:GetService("ReplicatedStorage"):WaitForChild("remotes"):WaitForChild("swing"):FireServer()
                        local v10 = {
                            v9:FindFirstChild("Humanoid"),
                            math.huge,
                            {},
                            0
                        }
                        game:GetService("ReplicatedStorage").remotes.onHit:FireServer(unpack(v10))
                    end
                end
                wait(0.5)
            end)
        end
    end)
end
v2:AddToggle({
    text = "Auto Mobs",
    flag = "toggle",
    state = false,
    callback = function(p11)
        _G.Mobs = p11
        print("Mobs: ", p11)
        if p11 then
            workspace.Gravity = 0
            _G.Mobs = true
            local vu12 = game:GetService("TweenService")
            local v13 = game:GetService("Players").LocalPlayer
            local vu14 = (v13.Character or v13.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
            while _G.Mobs do
                wait()
                pcall(function()
                    local v22 = (function()
                        local v15 = math.huge
                        local v16, v17, v18 = pairs(workspace:GetChildren())
                        local v19 = nil
                        while true do
                            local v20
                            v18, v20 = v16(v17, v18)
                            if v18 == nil then
                                break
                            end
                            if v20:IsA("Model") and (v20:FindFirstChild("HumanoidRootPart") and (v20:GetAttribute("hadEntrance") == true or v20:GetAttribute("hadEntrance") == false)) then
                                local v21 = (v20.HumanoidRootPart.Position - vu14.Position).Magnitude
                                if v21 < v15 then
                                    v19 = v20
                                    v15 = v21
                                end
                            end
                        end
                        return v19
                    end)()
                    if v22 and v22:FindFirstChild("HumanoidRootPart") then
                        local v23 = (v22.HumanoidRootPart.Position - vu14.Position).Magnitude
                        local v24 = v22.HumanoidRootPart.CFrame * CFrame.new(0, 0, 15)
                        local v25 = vu12:Create(vu14, TweenInfo.new(v23 / 50, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
                            CFrame = v24
                        })
                        v25:Play()
                        v25.Completed:Wait()
                    end
                end)
            end
        else
            _G.Mobs = false
            Wait()
            workspace.Gravity = 196
        end
    end
})

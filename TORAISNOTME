function Escape()
    spawn(function()
        _G.Escape = true
        while _G.Escape do
            wait()
            pcall(function()
                local v38, v39, v40 = pairs(workspace:GetDescendants())
                while true do
                    local v41
                    v40, v41 = v38(v39, v40)
                    if v40 == nil then
                        break
                    end
                    if v41.Name == "RadioObjective" and v41.ProximityPrompt.Enabled == true then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v41.CFrame
                        wait(0.2)
                        fireproximityprompt(v41.ProximityPrompt)
                    end
                end
                if game:GetService("Players").LocalPlayer.PlayerGui.MainScreen.ObjectiveDisplay.ObjectiveElement.List.Description.Text == "ESCAPE" then
                    _G.Mobs = false
                    _G.Coins = false
                    wait(0.5)
                    local v42, v43, v44 = pairs(workspace:GetDescendants())
                    while true do
                        local v45
                        v44, v45 = v42(v43, v44)
                        if v44 == nil then
                            break
                        end
                        if v45.Name == "HeliObjective" and v45.ProximityPrompt.Enabled == true then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v45.CFrame
                            wait(0.2)
                            fireproximityprompt(v45.ProximityPrompt)
                        end
                    end
                end
                local v46, v47, v48 = pairs(workspace:GetDescendants())
                while true do
                    local v49
                    v48, v49 = v46(v47, v48)
                    if v48 == nil then
                        break
                    end
                    if v49.Name == "gen" and v49.pom.Enabled == true then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v49.CFrame
                        wait(0.2)
                        fireproximityprompt(v49.pom)
                    end
                end
                wait(2)
            end)
        end
    end)

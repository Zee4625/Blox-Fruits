function Attack()
    spawn(function()
        while _G.Attack do
            task.wait(0.1)
            pcall(function()
                local v6 = os.time() + tick() % 1
                local v7 = {
                    buffer.fromstring("\8\4\1"),
                    {
                        v6 + vu3
                    }
                }
                game:GetService("ReplicatedStorage"):WaitForChild("ByteNetReliable"):FireServer(unpack(v7))
            end)
        end
    end)
end

            local v14 = true
            while true do
                local v15
                v13, v15 = v11(v12, v13)
                if v13 == nil then
                    break
                end
                if v15:IsA("Model") and (v15:GetAttribute("roomIndex") and not v15:GetAttribute("activated")) then
                    v14 = false
                    break
                end
            end
        until v14 or not _G.Mobs
        while true do
            task.wait()
            local v16 = vu10.Character
            if v16 then
                v16 = vu10.Character:FindFirstChild("HumanoidRootPart")
            end
            local v17, v18, v19 = pairs(workspace.Entities:GetDescendants())
            local v20
            v19, v20 = v17(v18, v19)
            if v19 ~= nil then
                if v20.Name == "HumanoidRootPart" and (v20.Anchored == true and v16) then
                    v16.CFrame = v20.CFrame * CFrame.new(0, Distance, 0)
                end
            end
            if not _G.Mobs then
                return
            end
        end
    end)
end

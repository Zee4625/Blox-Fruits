function Box()
    spawn(function()
        _G.Box = true
        while _G.Box do
            wait()
            pcall(function()
                local v4, v5, v6 = pairs(workspace.World.Items:GetDescendants())
                while true do
                    local v7
                    v6, v7 = v4(v5, v6)
                    if v6 == nil then
                        break
                    end
                    if string.find(v7.Name, "Box") and v7:GetAttribute("Water") == true or string.find(v7.Name, "Chest") and v7:GetAttribute("Water") == true then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v7.Position) * CFrame.new(0, 5, 0)
                        game:GetService("ReplicatedStorage").Action:InvokeServer(unpack({
                            "Collect",
                            v7
                        }))
                        break
                    end
                end
                wait()
            end)
        end
    end)
end

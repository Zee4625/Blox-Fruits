local vu19 = nil
local vu20 = nil
function noclip()
    vu20 = false
    local function v25()
        if vu20 == false and game.Players.LocalPlayer.Character ~= nil then
            local v21, v22, v23 = pairs(game.Players.LocalPlayer.Character:GetDescendants())
            while true do
                local v24
                v23, v24 = v21(v22, v23)
                if v23 == nil then
                    break
                end
                if v24:IsA("BasePart") and (v24.CanCollide and v24.Name ~= floatName) then
                    v24.CanCollide = false
                end
            end
        end
        wait(0.21)
    end
    vu19 = game:GetService("RunService").Stepped:Connect(v25)
end
function clip()
    if vu19 then
        vu19:Disconnect()
    end
    vu20 = true
end
noclip()

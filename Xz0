    {Title = "Tp Cake Land", Position = Vector3.new(-16133.21, 31.60, 1037.90)} 
}

for _, location in pairs(locations) do
    TabTP:AddButton({
        Title = location.Title,
        Tip = "Click to teleport",
        Callback = function()
            teleportTo(location.Position)
        end
    })
end


-- Tab-ul Farm pentru activarea func»õiei de farmare a cuferelelor
local TabFarm = Window:AddTab({Title = "FARM", Icon = "gift"})

TabFarm:AddToggle({
    Title = "Farm Chest",
    Tip = "Farm Chests",
    Default = false,
    Callback = function(value)
        if value then
            print("Activat farm de cufere")
            flyThroughAllChests()  -- ActiveazƒÉ farm-ul de cufere
        else
            farmActive = false  -- Opre»ôte farm-ul dacƒÉ este dezactivat
            print("Dezactivat farm de cufere")
        end
    end,
})



-- Tab for Range toggle
TabFarm:AddToggle({
    Title = "Range",
    Tip = "Increase hand size and objects",
    Default = false,
    Callback = function(value)
        if value then
            activateRange()
        else
            deactivateRange()
        end
    end,
})


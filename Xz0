-- Tab ESP for X-ray and Fruit ESP
local TabESP = Window:AddTab({Title = "ESP", Icon = "eye"})

TabESP:AddToggle({
    Title = "X-ray",
    Tip = "Highlight Players",
    Default = false,
    Callback = function(value)
        xrayEnabled = value
        if xrayEnabled then activateXRay() end
    end,
})

-- Toggling pentru Fruit ESP
TabESP:AddToggle({
    Title = "Fruit ESP",
    Tip = "Highlight Fruits",
    Default = false,
    Callback = function(value)
        fruitESPEnabled = value
        if fruitESPEnabled then
            -- DacƒÉ activƒÉm ESP, apelƒÉm func»õia corespunzƒÉtoare.
            activateFruitESP()
        else
            -- DacƒÉ dezactivƒÉm ESP, apelƒÉm func»õia de dezactivare.
            deactivateFruitESP()
        end
    end,
})

local function GetOnlinePlayers()
    -- Func»õie care returneazƒÉ o listƒÉ cu to»õi jucƒÉtorii online
    local players = {}  
    for _, player in pairs(game.Players:GetPlayers()) do
        table.insert(players, player.Name)
    end
    return players
end

local Dropdown

local function UpdateDropdown()
    if Dropdown then
        Dropdown:SetValues(GetOnlinePlayers())
    end
end

Dropdown = TabESP:AddDropdown({
    Title = "Spy",
    Values = GetOnlinePlayers(),
    Default = "",
    Multi = false,
    MaxMulti = 1,
    Callback = function(value)
        local targetPlayer = game.Players:FindFirstChild(value)
        if targetPlayer then
            -- Cod pentru a schimba perspectiva la jucƒÉtorul selectat
            game.Workspace.CurrentCamera.CameraSubject = targetPlayer.Character:FindFirstChild("Humanoid")
        end
    end,
})

-- Actualizare lista de jucƒÉtori la fiecare secundƒÉ
task.spawn(function()
    while true do
        UpdateDropdown()
        task.wait(1)
    end
end)



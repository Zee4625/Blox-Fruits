titleLabel.TextColor3 = Color3.fromRGB(220,235,255)
titleLabel.Font = Enum.Font.RobotoMono
titleLabel.TextSize = 18
titleLabel.TextXAlignment = Enum.TextXAlignment.Left
titleLabel.Size = UDim2.new(0.7,0,1,0)

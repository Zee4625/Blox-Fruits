local Notifications = loadstring(game:HttpGet("https://raw.githubusercontent.com/tacolift/notification-library/main/library.lua"))()
Notifications.Create("ZeeHUB v10", "Wellcome to ZeeHUB v10!", 3, Color3.fromRGB(255,0,250))
-- First argument is the title, second one's the description, third one's the time and the last one's the title color.
task.wait(2)
Notifications.Clear()

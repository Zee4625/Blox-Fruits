        local vu48 = nil
        local vu49 = 0
        while _G.Mobs2 do
            wait()
            pcall(function()
                local vu50 = game:GetService("Players").LocalPlayer
                local v59 = (function()
                    local v51 = math.huge
                    local v52, v53, v54 = pairs(workspace.__Main.__Enemies.Client:GetDescendants())
                    local v55 = nil
                    while true do
                        local v56
                        v54, v56 = v52(v53, v54)
                        if v54 == nil then
                            break
                        end
                        if v56.Name == "HealthBar" and v56.Main.Title.Text == mobsname then
                            local v57 = v56.Main.Bar.Amount.Text
                            if v57 == "0 HP" then
                                if v57 == "0 HP" then
                                    wait(1)
                                end
                            else
                                local v58 = vu50:DistanceFromCharacter(v56.Parent.HumanoidRootPart.CFrame.p)
                                if v58 < v51 then
                                    v55 = v56
                                    v51 = v58
                                end
                            end
                        end
                    end
                    return v55
                end)()
                if v59 and v59 ~= vu48 then
                    vu48 = v59
                    vu49 = tick()

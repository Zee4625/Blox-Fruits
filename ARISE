function Attack()
    spawn(function()
        _G.Attack = true
        local vu6 = nil
        while _G.Attack do
            wait()
            pcall(function()
                local vu7 = game:GetService("Players").LocalPlayer
                local v8 = workspace.__Main.__Pets[vu7.UserId]
                local v9, v10, v11 = ipairs(v8:GetChildren())
                local v12 = {}
                while true do
                    local v13
                    v11, v13 = v9(v10, v11)
                    if v11 == nil then
                        break
                    end
                    if v13:IsA("Model") and v13:FindFirstChild("PrimaryPart") then
                        v12[v13.Name] = v13.PrimaryPart.Position
                    end
                end
                local function v21()
                    local v14 = math.huge
                    local v15, v16, v17 = pairs(workspace.__Main.__Enemies.Server:GetDescendants())
                    local v18 = nil
                    while true do
                        local v19
                        v17, v19 = v15(v16, v17)
                        if v17 == nil then
                            break
                        end
                        if v19:IsA("Part") and (v19:GetAttribute("HP") and v19:GetAttribute("HP") > 0) then
                            local v20 = vu7:DistanceFromCharacter(v19.CFrame.p)
                            if v20 < v14 then
                                v18 = v19
                                v14 = v20
                            end
                        end
                    end
                    return v18
                end
                local v22 = v21()
                if vu6 and (vu6:GetAttribute("HP") and vu6:GetAttribute("HP") <= 0) then
                    local v23 = {
                        {
                            {
                                Event = vu3,
                                Enemy = vu6.Name
                            },
                            "\4"
                        }
                    }
                    game:GetService("ReplicatedStorage").BridgeNet2.dataRemoteEvent:FireServer(unpack(v23))
                    vu6 = nil
                    v22 = v21()
                    task.wait(0.5)
                end
                if v22 and v22 ~= vu6 then
                    task.wait(1)
                    for _ = 1, 2 do
                        local v24 = {
                            {
                                {
                                    PetPos = v12,
                                    AttackType = "All",
                                    Event = "Attack",
                                    Enemy = v22.Name
                                },
                                "\7"
                            }
